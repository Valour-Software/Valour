  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Shared\Valour.Shared.csproj" />
  </ItemGroup>

  <ItemGroup>
    <ServiceWorker Include="wwwroot\service-worker.js" PublishedContent="wwwroot\service-worker.published.js" />
  </ItemGroup>

</Project>
